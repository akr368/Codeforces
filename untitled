#include <iostream>
#include <string>


using namespace std;

int main(){
 
 string s;
 cin>>s;
 int nOne=0;
 int nTwo=0;
 int nThree=0;
 int i;
 for(i=0;i<s.size();i+=2){
   
   if(s[i]=='1')
    nOne++;
   else if(s[i]=='2')
    nTwo++;
   else
    nThree++;

 }

 cout<<nOne<<" "<<nTwo<<" "<<nThree<<endl;

 return 0;

}